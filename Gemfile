# Edit this Gemfile to bundle your application's dependencies.

## Bundle edge rails:
gem "rails", :git => "git://github.com/rails/rails.git"
gem "arel", :git => "git://github.com/rails/arel.git"

gem "mysql"
gem "mongoid"

only :test do
  gem "rspec", '>= 2.0.0.a2'
  gem "rspec-rails", '>= 2.0.0.a2'
  gem "webrat"
end
